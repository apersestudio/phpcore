{
    "name": "apersestudio/phpcore",
    "description": "PHP minimalistic framework",
    "type": "framework",
    "version":"1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PC\\": "src/",
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Jaime Zaballa Espinosa",
            "email": "muchologo@gmail.com"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "cp vendor/apersestudio/phpcore/composer.json ./composer.json",
            "PC\\Install\\PostInstallCmd::start"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "robinvdvleuten/ulid": "^5.0",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
